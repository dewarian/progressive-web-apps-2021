{
  "env": {
      "browser": true,
      "node": true,
      "es2021": true
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
    "no-implicit-globals":"warn",
    "space-before-function-paren": "off"
  },
  "plugins": ["prettier"]
}
